#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/bin/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/bin/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/graphviz-cli@2.0.0/node_modules:/Users/jack/OnboardX/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/bin/graphviz.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/graphviz-cli@2.0.0/node_modules/graphviz-cli/bin/graphviz.cjs" "$@"
fi
